{
  "database": {
    "type": "prisma",
    "provider": "postgresql"
  },
  "socialProviders": {
    "google": {
      "enabled": true
    },
    "github": {
      "enabled": true
    }
  },
  "accountLinking": {
    "enabled": true,
    "trustedProviders": [
      "google",
      "github",
      "email-password"
    ],
    "allowDifferentEmails": false
  },
  "rateLimit": {
    "enabled": true,
    "window": 60,
    "maxRequests": 100,
    "customRules": {}
  },
  "advanced": {
    "enabled": true,
    "useSecureCookies": true,
    "httpOnlyCookies": true,
    "secureCookies": true
  }
}